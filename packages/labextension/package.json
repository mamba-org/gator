{
  "name": "@mamba-org/gator-lab",
  "version": "3.0.0",
  "description": "Manage your conda environments from JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "conda",
    "mamba"
  ],
  "homepage": "https://github.com/mamba-org/gator",
  "bugs": {
    "url": "https://github.com/mamba-org/gator/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jupyter Development Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/mamba-org/gator.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:dev": "tsc",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:dev": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf ../../mamba_gator/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "install:extension": "jupyter labextension develop --overwrite .",
    "lab:link": "jupyter labextension link . --no-build",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "@jupyterlab/coreutils": "^5.0.0",
    "@jupyterlab/mainmenu": "^3.0.0",
    "@jupyterlab/services": "^6.0.0",
    "@jupyterlab/settingregistry": "^3.0.0",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/widgets": "^1.16.1",
    "@mamba-org/gator-common": "^3.0.0",
    "jupyterlab_toastify": "^4.1.3",
    "semver": "^6.0.0||^7.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.1",
    "@jupyterlab/testutils": "^3.0.0",
    "@types/react": "^17.0.0",
    "@types/semver": "^7.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "sideEffects": [
    "style/*"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "discovery": {
      "server": {
        "base": {
          "name": "mamba_gator"
        },
        "managers": [
          "conda"
        ]
      }
    },
    "outputDir": "../../mamba_gator/labextension"
  },
  "publishConfig": {
    "access": "public"
  }
}