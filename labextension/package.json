{
  "name": "jupyterlab_conda",
  "version": "2.1.0",
  "description": "Manage your conda environments from JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "conda"
  ],
  "homepage": "https://github.com/fcollonval/jupyter_conda",
  "bugs": {
    "url": "https://github.com/fcollonval/jupyter_conda/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jupyter Development Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcollonval/jupyter_conda.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "precommit": "pretty-quick --staged",
    "prepare": "jlpm run clean && jlpm run build",
    "prettier": "npx prettier --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "test": "jest",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "@jupyterlab/coreutils": "^4.0.0",
    "@jupyterlab/mainmenu": "^2.0.0",
    "@jupyterlab/services": "^5.0.0",
    "@jupyterlab/settingregistry": "^2.0.1",
    "@jupyterlab/ui-components": "^2.0.0",
    "@lumino/coreutils": "^1.3.0",
    "@lumino/signaling": "^1.2.0",
    "@lumino/widgets": "^1.6.0",
    "jupyterlab_toastify": "^3.0.0",
    "react-virtualized": "^9.21.1",
    "semver": "^6.3.0",
    "typestyle": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@jupyterlab/testutils": "^1.2.2",
    "@types/jest": "^25",
    "@types/react": "~16.9.0",
    "@types/react-virtualized": "^9.21.4",
    "@types/semver": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^0.14.0",
    "jest": "^25.0.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-raw-loader": "^1.0.1",
    "prettier": "~1.14.2",
    "pretty-quick": "~1.6.0",
    "react": "~16.9.0",
    "rimraf": "~2.6.1",
    "ts-jest": "^25.0.0",
    "typescript": "~3.7.3"
  },
  "sideEffects": [
    "style/*"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "discovery": {
      "server": {
        "base": {
          "name": "jupyter_conda"
        },
        "managers": [
          "conda"
        ]
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "@types/react": "~16.9.0"
  }
}
